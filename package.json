{
  "name": "vscode-inline-webassembly",
  "displayName": "vscode-inline-webassembly",
  "description": "WebAssembly syntax highlighting for JS and TS string templates",
  "version": "1.0.0",
  "publisher": "dorin131",
	"license": "MIT",
  "icon": "images/logo.png",
  "repository": {
		"type": "git",
		"url": "https://github.com/dorin131/vscode-inline-webassembly"
	},
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
		"grammars": [
      {
        "scopeName": "source.wasm",
        "language": "webassembly",
        "path": "./syntaxes/webassembly.tmLanguage.json"
      },
			{
				"injectTo": [
					"source.js",
					"source.ts"
				],
				"scopeName": "source.wasm.inline",
				"path": "./syntaxes/inline-webassembly.tmLanguage.json",
				"embeddedLanguages": {
					"source.wasm": "webassembly"
				}
			}
		]
	}
}